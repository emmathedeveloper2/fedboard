{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fedboard",
  "version": "0.3.0",
  "identifier": "com.emmathedeveloper.fedboard",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fedboard",
        "width": 300,
        "height": 400,
        "decorations": false,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": ["https://github.com/emmathedeveloper2/fedboard/releases/latest/download/latest.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIzOTM0OTJCODYyQTUyNEQKUldSTlVpcUdLMG1USTFUcTlMZU9oc08xUENOaWxQZTJXNjNLOXgxK2tVRnR1VEpMNUdrWVpSY2QK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
